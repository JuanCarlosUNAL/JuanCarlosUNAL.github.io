    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "gatsby": "^3.0.1",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-mdx": "^2.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-react-intl": "^3.0.0",
    "gatsby-plugin-sass": "^4.1.0",
